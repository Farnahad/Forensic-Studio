<userControl:FsUserControl
    x:Class="ForensicStudio.Core.Control.UserControl.ButtonSet.FsExecuteClearButtonSetUserControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:userControl="clr-namespace:ForensicStudio.Core.Control.UserControl"
    xmlns:container="clr-namespace:ForensicStudio.Core.Control.Container"
    xmlns:button="clr-namespace:ForensicStudio.Core.Control.Button"
    xmlns:fsmc="ForensicStudioMainCore">
    <container:FsWrapPanel HorizontalAlignment="Center">
        <button:FsSimpleButton Content="Execute" Command="{fsmc:FsBinding Path=ExecuteCommand}" />
        <button:FsSimpleButton Content="Clear" Command="{fsmc:FsBinding Path=ClearCommand}" />
    </container:FsWrapPanel>
</userControl:FsUserControl>