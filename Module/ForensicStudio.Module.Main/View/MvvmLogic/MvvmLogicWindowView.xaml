<fscm:GeneralView x:Class="ForensicStudio.Module.Main.View.MvvmLogic.MvvmLogicWindowView"
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                  xmlns:fscm="ForensicStudioCoreModule"
                  xmlns:fsc="ForensicStudioControls"
                  xmlns:fsmc="ForensicStudioMainCore"
                  mc:Ignorable="d"
                  d:DesignHeight="450" d:DesignWidth="800">
    <fsc:FsLayoutControl>
        <fsc:FsVerticalLayoutGroup>
            <fsc:FsLayoutItem VerticalAlignment="Stretch">
                <fsc:FsListBoxEdit ItemsSource="{fsmc:FsBinding Path=Logs}" />
            </fsc:FsLayoutItem>
            <fsc:FsLayoutItem VerticalAlignment="Center">
                <fsc:FsSimpleButton Content="Close" Command="{fsmc:FsBinding Path=CloseCommand}" />
            </fsc:FsLayoutItem>
        </fsc:FsVerticalLayoutGroup>
    </fsc:FsLayoutControl>
</fscm:GeneralView>